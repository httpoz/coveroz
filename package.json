        "element-theme-default": "^1.4.13",
        "feather-icons": "^4.7.0",
        "jquery": "^3.3",
        "laravel-mix": "^2.1",
        "lodash": "^4.17.10",
        "popper.js": "^1.14",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.7",
        "redux": "^4.0.0",
        "redux-devtools-extension": "^2.13.2",
        "redux-thunk": "^2.2.0"
